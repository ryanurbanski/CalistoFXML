<AnchorPane id="AnchorPane" prefHeight="565.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ApplicationListController">
   <children>
      <BorderPane prefHeight="563.0" prefWidth="320.0">
         <center>
            <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox prefHeight="48.0" prefWidth="320.0">
                     <children>
                        <Label alignment="CENTER" prefHeight="48.0" prefWidth="258.0" text="                       Pending Applications" />
                        <Button mnemonicParsing="false" onAction="#handleSignOutButtonAction" prefHeight="48.0" prefWidth="76.0" text="Sign Out" />
                     </children>
                  </HBox>
                  <ListView fx:id="listView" prefHeight="363.0" prefWidth="320.0" />
                  <Button mnemonicParsing="false" onAction="#handleViewApplicationButtonAction" prefHeight="27.0" prefWidth="326.0" text="View Application" />
               </children>
            </VBox>
         </center>
      </BorderPane>
   </children>
