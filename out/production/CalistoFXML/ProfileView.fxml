<AnchorPane id="AnchorPane" prefHeight="580.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ProfileViewController">
   <children>
      <BorderPane prefHeight="580.0" prefWidth="320.0">
